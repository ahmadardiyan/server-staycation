<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>No</th>
            <th>Tanggal</th>
            <th>Judul</th>
            <th>Nama Member</th>
            <th>Nama Bank</th>
            <th>Status</th>
            <th>Aksi</th>
          </tr>
        </thead>
        <tbody>
          <% for (let i = 0; i < booking.length; i++) { %>
            <tr>
              <td><%= i + 1 %></td>
              <td><%= booking[i].startDate.toLocaleString().split(',')[0] %> s/d <%= booking[i].endDate.toLocaleString().split(',')[0] %></td>
              <td><%= booking[i].itemId.title %></td>
              <td><%= booking[i].memberId.firstName %> <%= booking[i].memberId.lastName %></td>
              <td><%= booking[i].payments.bankFrom %></td>
              <td><%= booking[i].payments.status %></td>
              <td>
                <a href="/admin/booking/<%= booking[i]._id %>" type="button" class="btn btn-success btn-sm button-update"><i class="fas fa-eye"></i></a>
              </td>
            </tr>
            <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>