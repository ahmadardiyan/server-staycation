<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="show-image-tab">
  <div class="row">
    <% for(let i = 0; i < items.imageId.length; i++) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img src="http://localhost:3000/<%= items.imageId[i].imageUrl %>" alt="image" style="width: 100%; display: block">
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a href="/admin/item/" type="button" class="btn btn-info btn-sm button-update" >Back</a>
</div>